fname = "mbox-short.txt"
fhand = open(fname)

hrs = dict()
for line in fhand:
    if line.startswith('From '):
        words = line.split()
        hour = words[5].split(":")[0]
        hrs[hour]= hrs.get(hour , 0) + 1
        
lst = list()
for hours in hrs:
    lst.append((hours , hrs[hours]))
    lst.sort()
    
for hour , count in lst:
    print hour , count